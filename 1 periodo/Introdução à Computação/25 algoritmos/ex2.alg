algoritmo "exercicio2"
var
   numero1,numero2,numero3,total:inteiro
inicio
   escreva("Escreva o número 1: ")
   leia(numero1)
   escreva("Escreva o número 2: ")
   leia(numero2)
   escreva("Escreva o número 3: ")
   leia(numero3)
   total <- numero1 * numero2 * numero3
   escreva("O total é: ", total)
fimalgoritmo
