algoritmo "exercicio12"
var
   salario,salario_minimo:real
inicio
   escreva("Escreva o salário mínimo: ")
   leia(salario_minimo)
   escreva("Escreva o seu salário: ")
   leia(salario)
   escreva("Você recebe ", salario / salario_minimo)
   escreva(" salário(s) mínimo(s).")
fimalgoritmo
