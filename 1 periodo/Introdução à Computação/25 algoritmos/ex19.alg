algoritmo "exercicio19"
var
   lado1,lado2,area:real
inicio
   escreva("Escreva o lado 1 do cômodo (em metros): ")
   leia(lado1)
   escreva("Escreva o lado 2 do cômodo (em metros): ")
   leia(lado2)
   area <- lado1 * lado2
   escreval("A área do cômodo é: ", area)
   escreva("A potência necessária é: ", area * 18)
   escreva("w")
fimalgoritmo
